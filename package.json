{
  "name": "sagebox-icon-studio",
  "displayName": "SageBox Icon Studio",
  "description": "Professional SVG icon management studio. Preview, edit, transform and build icon components for your design system.",
  "version": "0.1.0",
  "publisher": "sagebox",
  "author": {
    "name": "Adrián Dávila Guerra",
    "url": "https://adravilag.github.io/sagebox/"
  },
  "license": "MIT",
  "homepage": "https://adravilag.github.io/sagebox/components/icon/",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "keywords": [
    "svg",
    "icons",
    "icon studio",
    "sagebox",
    "svg components",
    "iconify",
    "sprites",
    "react icons",
    "vue icons",
    "design system"
  ],
  "activationEvents": [
    "workspaceContains:**/*.svg",
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:svelte",
    "onLanguage:astro"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "sageboxIconStudio.svgMenu",
        "label": "Icon Studio"
      }
    ],
    "languages": [
      {
        "id": "sageboxignore",
        "aliases": [
          "SageBox Ignore"
        ],
        "filenames": [
          ".sageboxignore"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sageboxignore",
        "scopeName": "source.sageboxignore",
        "path": "./syntaxes/sageboxignore.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "sageboxIconStudio.openPanel",
        "title": "%command.openPanel%",
        "icon": "$(symbol-color)"
      },
      {
        "command": "sageboxIconStudio.openWelcome",
        "title": "%command.openWelcome%",
        "icon": "$(home)"
      },
      {
        "command": "sageboxIconStudio.scanWorkspace",
        "title": "%command.scanWorkspace%"
      },
      {
        "command": "sageboxIconStudio.scanUsages",
        "title": "%command.scanUsages%",
        "icon": "$(references)"
      },
      {
        "command": "sageboxIconStudio.transformSvgToIcon",
        "title": "%command.transformSvgToIcon%"
      },
      {
        "command": "sageboxIconStudio.optimizeSvg",
        "title": "%command.optimizeSvg%",
        "icon": "$(rocket)"
      },
      {
        "command": "sageboxIconStudio.insertIcon",
        "title": "%command.insertIcon%"
      },
      {
        "command": "sageboxIconStudio.previewIcon",
        "title": "%command.previewIcon%"
      },
      {
        "command": "sageboxIconStudio.goToInlineSvg",
        "title": "%command.goToInlineSvg%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "sageboxIconStudio.goToUsage",
        "title": "%command.goToUsage%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "sageboxIconStudio.transformInlineSvg",
        "title": "%command.transformInlineSvg%",
        "icon": "$(replace)"
      },
      {
        "command": "sageboxIconStudio.configureProject",
        "title": "%command.configureProject%",
        "icon": "$(gear)"
      },
      {
        "command": "sageboxIconStudio.editIgnoreFile",
        "title": "%command.editIgnoreFile%",
        "icon": "$(list-filter)"
      },
      {
        "command": "sageboxIconStudio.refreshIcons",
        "title": "%command.refreshIcons%",
        "icon": "$(refresh)"
      },
      {
        "command": "sageboxIconStudio.refreshFiles",
        "title": "%command.refreshFiles%",
        "icon": "$(refresh)"
      },
      {
        "command": "sageboxIconStudio.refreshCode",
        "title": "%command.refreshCode%",
        "icon": "$(refresh)"
      },
      {
        "command": "sageboxIconStudio.refreshBuilt",
        "title": "%command.refreshBuilt%",
        "icon": "$(refresh)"
      },
      {
        "command": "sageboxIconStudio.buildIcons",
        "title": "%command.buildIcons%",
        "icon": "$(package)"
      },
      {
        "command": "sageboxIconStudio.searchIcons",
        "title": "%command.searchIcons%",
        "icon": "$(search)"
      },
      {
        "command": "sageboxIconStudio.deleteIcons",
        "title": "%command.deleteIcons%",
        "icon": "$(trash)"
      },
      {
        "command": "sageboxIconStudio.removeFromBuilt",
        "title": "%command.removeFromBuilt%",
        "icon": "$(remove)"
      },
      {
        "command": "sageboxIconStudio.showDetails",
        "title": "%command.showDetails%",
        "icon": "$(info)"
      },
      {
        "command": "sageboxIconStudio.goToCode",
        "title": "%command.goToCode%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "sageboxIconStudio.openSvgFile",
        "title": "%command.openSvgFile%",
        "icon": "$(file-media)"
      },
      {
        "command": "sageboxIconStudio.copyIconName",
        "title": "%command.copyIconName%",
        "icon": "$(copy)"
      },
      {
        "command": "sageboxIconStudio.colorEditor",
        "title": "%command.colorEditor%",
        "icon": "$(edit)"
      },
      {
        "command": "sageboxIconStudio.expandAll",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "sageboxIconStudio.collapseAll",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "sageboxIconStudio.expandBuiltIcons",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "sageboxIconStudio.collapseBuiltIcons",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "sageboxIconStudio.expandSvgFiles",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "sageboxIconStudio.collapseSvgFiles",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "sageboxIconStudio.configureSvgFolder",
        "title": "%command.configureSvgFolder%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "sageboxIconStudio.exportComponent",
        "title": "%command.exportComponent%",
        "icon": "$(file-code)"
      },
      {
        "command": "sageboxIconStudio.generateSprite",
        "title": "%command.generateSprite%",
        "icon": "$(layers)"
      },
      {
        "command": "sageboxIconStudio.addSvgToCollection",
        "title": "%command.addSvgToCollection%",
        "icon": "$(add)"
      },
      {
        "command": "sageboxIconStudio.importMissingIcon",
        "title": "%command.importMissingIcon%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "sageboxIconStudio.removeReference",
        "title": "%command.removeReference%",
        "icon": "$(trash)"
      },
      {
        "command": "sageboxIconStudio.findAndReplace",
        "title": "%command.findAndReplace%",
        "icon": "$(search-replace)"
      },
      {
        "command": "sageboxIconStudio.renameIcon",
        "title": "%command.renameIcon%",
        "icon": "$(edit)"
      },
      {
        "command": "sageboxIconStudio.viewSprite",
        "title": "%command.viewSprite%",
        "icon": "$(eye)"
      },
      {
        "command": "sageboxIconStudio.viewIconsFile",
        "title": "%command.viewIconsFile%",
        "icon": "$(eye)"
      },
      {
        "command": "sageboxIconStudio.deleteBuiltFile",
        "title": "%command.deleteBuiltFile%",
        "icon": "$(trash)"
      },
      {
        "command": "sageboxIconStudio.buildAllReferences",
        "title": "%command.buildAllReferences%",
        "icon": "$(package)"
      },
      {
        "command": "sageboxIconStudio.buildSingleIcon",
        "title": "%command.buildSingleIcon%",
        "icon": "$(package)"
      },
      {
        "command": "sageboxIconStudio.buildAllFiles",
        "title": "%command.buildAllFiles%",
        "icon": "$(package)"
      },
      {
        "command": "sageboxIconStudio.cleanSprite",
        "title": "%command.cleanSprite%",
        "icon": "$(trash)"
      },
      {
        "command": "sageboxIconStudio.generateLicenses",
        "title": "%command.generateLicenses%",
        "icon": "$(law)"
      },
      {
        "command": "sageboxIconStudio.showLicenseSummary",
        "title": "%command.showLicenseSummary%",
        "icon": "$(list-unordered)"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "sageboxIconStudio.language": {
          "type": "string",
          "enum": [
            "auto",
            "en",
            "es",
            "zh",
            "ru"
          ],
          "enumDescriptions": [
            "%config.language.auto%",
            "%config.language.en%",
            "%config.language.es%",
            "%config.language.zh%",
            "%config.language.ru%"
          ],
          "default": "auto",
          "description": "%config.language.description%"
        },
        "sageboxIconStudio.componentName": {
          "type": "string",
          "default": "iw-icon",
          "description": "%config.componentName.description%"
        },
        "sageboxIconStudio.componentImport": {
          "type": "string",
          "default": "@/components/ui/Icon",
          "description": "%config.componentImport.description%"
        },
        "sageboxIconStudio.svgFolders": {
          "type": "array",
          "default": [
            "svgs",
            "src/assets/icons",
            "src/icons",
            "public/icons",
            "assets/icons",
            "icons",
            "svg",
            "assets/svg"
          ],
          "description": "%config.svgFolders.description%"
        },
        "sageboxIconStudio.outputFormat": {
          "type": "string",
          "enum": [
            "jsx",
            "vue",
            "svelte",
            "astro",
            "html"
          ],
          "default": "jsx",
          "description": "Default output format for icon components"
        },
        "sageboxIconStudio.iconNameAttribute": {
          "type": "string",
          "default": "name",
          "description": "Attribute name for the icon identifier (name, icon, src, etc.)"
        },
        "sageboxIconStudio.autoImport": {
          "type": "boolean",
          "default": true,
          "description": "Automatically add import statement when inserting icons"
        },
        "sageboxIconStudio.libraryPath": {
          "type": "string",
          "default": "",
          "description": "Path to the Icon Studio library (icons.json)"
        },
        "sageboxIconStudio.outputDirectory": {
          "type": "string",
          "default": "",
          "description": "Directory where icons.ts or sprite.svg will be generated (relative to workspace root)"
        },
        "sageboxIconStudio.buildFormat": {
          "type": "string",
          "enum": [
            "icons.ts",
            "sprite.svg"
          ],
          "default": "",
          "description": "Output format for built icons"
        },
        "sageboxIconStudio.webComponentName": {
          "type": "string",
          "default": "",
          "description": "Name of the Web Component for icons (e.g. sg-icon)"
        },
        "sageboxIconStudio.deleteAfterBuild": {
          "type": "boolean",
          "default": false,
          "description": "Delete original SVG file after building to sprite/component"
        },
        "sageboxIconStudio.previewBackground": {
          "type": "string",
          "enum": [
            "transparent",
            "light",
            "dark",
            "checkered"
          ],
          "default": "checkered",
          "description": "Background style for icon preview"
        },
        "sageboxIconStudio.defaultIconSize": {
          "type": "number",
          "default": 24,
          "minimum": 8,
          "maximum": 512,
          "description": "Default size (in pixels) for generated icons"
        },
        "sageboxIconStudio.namingConvention": {
          "type": "string",
          "enum": [
            "kebab-case",
            "camelCase",
            "PascalCase",
            "snake_case"
          ],
          "default": "kebab-case",
          "description": "Naming convention for icon identifiers"
        },
        "sageboxIconStudio.svgoOptimize": {
          "type": "boolean",
          "default": true,
          "description": "Automatically optimize SVGs with SVGO when building"
        },
        "sageboxIconStudio.svgoRemoveViewBox": {
          "type": "boolean",
          "default": false,
          "description": "Remove viewBox attribute during SVGO optimization (not recommended)"
        },
        "sageboxIconStudio.svgoRemoveColors": {
          "type": "boolean",
          "default": false,
          "description": "Remove fill/stroke colors during optimization (uses currentColor)"
        },
        "sageboxIconStudio.defaultAnimation": {
          "type": "string",
          "enum": [
            "none",
            "spin",
            "pulse",
            "bounce",
            "shake",
            "fade"
          ],
          "default": "none",
          "description": "Default animation for new icons"
        },
        "sageboxIconStudio.showUsagesInTree": {
          "type": "boolean",
          "default": true,
          "description": "Show usage count next to icons in tree view"
        },
        "sageboxIconStudio.scanOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically scan workspace for SVG files on startup"
        },
        "sageboxIconStudio.licenseFormat": {
          "type": "string",
          "enum": [
            "combined",
            "perCollection",
            "both"
          ],
          "default": "combined",
          "description": "%config.licenseFormat.description%"
        },
        "sageboxIconStudio.autoGenerateLicenses": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoGenerateLicenses.description%"
        },
        "sageboxIconStudio.licensesFolder": {
          "type": "string",
          "default": "icon-licenses",
          "description": "%config.licensesFolder.description%"
        },
        "sageboxIconStudio.excludePatterns": {
          "type": "array",
          "default": [
            "**/node_modules/**",
            "**/dist/**",
            "**/build/**",
            "**/.git/**"
          ],
          "description": "Glob patterns to exclude from SVG scanning"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sageboxIconStudio",
          "title": "Icon Studio",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "sageboxIconStudio": [
        {
          "id": "sageboxIconStudio.svgFiles",
          "name": "%view.sageboxFiles.title%"
        },
        {
          "id": "sageboxIconStudio.workspaceIcons",
          "name": "%view.sageboxCode.title%"
        },
        {
          "id": "sageboxIconStudio.builtIcons",
          "name": "%view.sageboxBuilt.title%"
        },
        {
          "id": "sageboxIconStudio.preview",
          "name": "%view.sageboxPreview.title%",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sageboxIconStudio.configureSvgFolder",
          "when": "view == sageboxIconStudio.svgFiles",
          "group": "navigation@-1"
        },
        {
          "command": "sageboxIconStudio.expandSvgFiles",
          "when": "view == sageboxIconStudio.svgFiles && !sageboxIconStudio.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.collapseSvgFiles",
          "when": "view == sageboxIconStudio.svgFiles && sageboxIconStudio.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.editIgnoreFile",
          "when": "view == sageboxIconStudio.svgFiles",
          "group": "navigation@1"
        },
        {
          "command": "sageboxIconStudio.buildAllFiles",
          "when": "view == sageboxIconStudio.svgFiles",
          "group": "navigation@2"
        },
        {
          "command": "sageboxIconStudio.refreshIcons",
          "when": "view == sageboxIconStudio.svgFiles",
          "group": "navigation@2"
        },
        {
          "command": "sageboxIconStudio.expandAll",
          "when": "view == sageboxIconStudio.workspaceIcons && !sageboxIconStudio.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.collapseAll",
          "when": "view == sageboxIconStudio.workspaceIcons && sageboxIconStudio.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.refreshIcons",
          "when": "view == sageboxIconStudio.workspaceIcons",
          "group": "navigation@1"
        },
        {
          "command": "sageboxIconStudio.buildAllReferences",
          "when": "view == sageboxIconStudio.workspaceIcons",
          "group": "navigation@2"
        },
        {
          "command": "sageboxIconStudio.expandBuiltIcons",
          "when": "view == sageboxIconStudio.builtIcons && !sageboxIconStudio.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.collapseBuiltIcons",
          "when": "view == sageboxIconStudio.builtIcons && sageboxIconStudio.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "sageboxIconStudio.refreshBuilt",
          "when": "view == sageboxIconStudio.builtIcons",
          "group": "navigation@1"
        },
        {
          "command": "sageboxIconStudio.searchIcons",
          "when": "view == sageboxIconStudio.builtIcons",
          "group": "navigation@2"
        },
        {
          "command": "sageboxIconStudio.configureProject",
          "when": "view == sageboxIconStudio.builtIcons",
          "group": "navigation@3"
        }
      ],
      "editor/context": [
        {
          "command": "sageboxIconStudio.transformSvgToIcon",
          "when": "editorHasSelection",
          "group": "sageboxIconStudio"
        },
        {
          "command": "sageboxIconStudio.optimizeSvg",
          "when": "editorHasSelection",
          "group": "sageboxIconStudio"
        },
        {
          "command": "sageboxIconStudio.previewIcon",
          "when": "editorTextFocus",
          "group": "sageboxIconStudio"
        }
      ],
      "explorer/context": [
        {
          "command": "sageboxIconStudio.scanWorkspace",
          "when": "explorerResourceIsFolder",
          "group": "sageboxIconStudio"
        },
        {
          "submenu": "sageboxIconStudio.svgMenu",
          "when": "resourceExtname == .svg",
          "group": "sageboxIconStudio"
        }
      ],
      "sageboxIconStudio.svgMenu": [
        {
          "command": "sageboxIconStudio.colorEditor",
          "when": "sageboxIconStudio.iconsJsExists",
          "group": "1_edit@1"
        },
        {
          "command": "sageboxIconStudio.showDetails",
          "group": "1_edit@2"
        }
      ],
      "view/item/context": [
        {
          "command": "sageboxIconStudio.addSvgToCollection",
          "when": "view == sageboxIconStudio.svgFiles && viewItem == svgIcon",
          "group": "inline"
        },
        {
          "command": "sageboxIconStudio.removeFromBuilt",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "inline"
        },
        {
          "command": "sageboxIconStudio.colorEditor",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/ && sageboxIconStudio.iconsJsExists",
          "group": "0_open@1"
        },
        {
          "command": "sageboxIconStudio.showDetails",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@2"
        },
        {
          "command": "sageboxIconStudio.openSvgFile",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "sageboxIconStudio.goToCode",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "sageboxIconStudio.copyIconName",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "sageboxIconStudio.exportComponent",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "sageboxIconStudio.renameIcon",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "7_modification"
        },
        {
          "command": "sageboxIconStudio.deleteIcons",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "sageboxIconStudio.removeFromBuilt",
          "when": "view == sageboxIconStudio.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "9_delete@2"
        },
        {
          "command": "sageboxIconStudio.transformInlineSvg",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef/",
          "group": "inline"
        },
        {
          "command": "sageboxIconStudio.goToInlineSvg",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|inlineSvgRasterized|inlineSvgRasterizedBuilt|inlineSvgBuilt|imgRef|imgRefRasterized/",
          "group": "0_open@1"
        },
        {
          "command": "sageboxIconStudio.colorEditor",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/ && sageboxIconStudio.iconsJsExists",
          "group": "0_open@2"
        },
        {
          "command": "sageboxIconStudio.showDetails",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "sageboxIconStudio.goToCode",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "sageboxIconStudio.copyIconName",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "sageboxIconStudio.exportComponent",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "sageboxIconStudio.deleteIcons",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "sageboxIconStudio.removeFromBuilt",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem =~ /inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "9_delete@2"
        },
        {
          "command": "sageboxIconStudio.goToInlineSvg",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == missingRef",
          "group": "0_open@1"
        },
        {
          "command": "sageboxIconStudio.removeReference",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == missingRef",
          "group": "1_fix@1"
        },
        {
          "command": "sageboxIconStudio.findAndReplace",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == missingRef",
          "group": "1_fix@2"
        },
        {
          "command": "sageboxIconStudio.goToInlineSvg",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == iconUsageMissing",
          "group": "0_open@1"
        },
        {
          "command": "sageboxIconStudio.importMissingIcon",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == iconUsageMissing",
          "group": "inline"
        },
        {
          "command": "sageboxIconStudio.importMissingIcon",
          "when": "view == sageboxIconStudio.workspaceIcons && viewItem == iconUsageMissing",
          "group": "1_fix@1"
        },
        {
          "command": "sageboxIconStudio.colorEditor",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtIcon && sageboxIconStudio.iconsJsExists",
          "group": "0_open@1"
        },
        {
          "command": "sageboxIconStudio.showDetails",
          "when": "view == sageboxIconStudio.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "0_open@2"
        },
        {
          "command": "sageboxIconStudio.copyIconName",
          "when": "view == sageboxIconStudio.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "1_copy@1"
        },
        {
          "command": "sageboxIconStudio.exportComponent",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtIcon",
          "group": "1_copy@2"
        },
        {
          "command": "sageboxIconStudio.renameIcon",
          "when": "view == sageboxIconStudio.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "7_modification"
        },
        {
          "command": "sageboxIconStudio.removeFromBuilt",
          "when": "view == sageboxIconStudio.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "9_delete"
        },
        {
          "command": "sageboxIconStudio.viewSprite",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtSpriteFile",
          "group": "0_open"
        },
        {
          "command": "sageboxIconStudio.deleteBuiltFile",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtSpriteFile",
          "group": "9_delete"
        },
        {
          "command": "sageboxIconStudio.viewIconsFile",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtIconsFile",
          "group": "0_open"
        },
        {
          "command": "sageboxIconStudio.deleteBuiltFile",
          "when": "view == sageboxIconStudio.builtIcons && viewItem == builtIconsFile",
          "group": "9_delete"
        }
      ]
    },
    "keybindings": [
      {
        "command": "sageboxIconStudio.openPanel",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      },
      {
        "command": "sageboxIconStudio.insertIcon",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node -e \"const fs=require('fs');const path=require('path');const dir='.vscode-test';if(fs.existsSync(dir))fs.rmSync(dir,{recursive:true,force:true})\" && npm run package:prod",
    "compile": "tsc -p ./ && npm run copy-templates",
    "compile:e2e": "tsc -p ./src/test/e2e/tsconfig.json && npm run copy-e2e-workspace",
    "copy-e2e-workspace": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)})};copyDir('src/test/e2e/test-workspace','out/test/e2e/test-workspace')\"",
    "copy-templates": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)})};copyDir('src/templates','out/templates')\"",
    "watch": "tsc -watch -p ./",
    "watch:esbuild": "node esbuild.js --watch",
    "package:prod": "node esbuild.js --production",
    "pretest": "npm run compile",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "npm run compile && npm run compile:e2e && node ./out/test/e2e/runTest.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "package": "vsce package"
  },
