{
  "name": "mastersvg-icon-studio",
  "displayName": "MasterSVG Icon Studio",
  "description": "Professional SVG icon management studio. Preview, edit, transform and build icon components for your design system.",
  "version": "0.1.0",
  "publisher": "MasterSVG",
  "author": {
    "name": "Adrián Dávila Guerra",
    "url": "https://adravilag.github.io/MasterSVG/"
  },
  "license": "MIT",
  "homepage": "https://adravilag.github.io/MasterSVG/components/icon/",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "keywords": [
    "svg",
    "icons",
    "icon studio",
    "MasterSVG",
    "svg components",
    "iconify",
    "sprites",
    "react icons",
    "vue icons",
    "design system"
  ],
  "activationEvents": [
    "workspaceContains:**/*.svg",
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:svelte",
    "onLanguage:astro"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "masterSVG.svgMenu",
        "label": "Icon Studio"
      }
    ],
    "languages": [
      {
        "id": "msignore",
        "aliases": [
          "MasterSVG Ignore"
        ],
        "filenames": [
          ".msignore"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "msignore",
        "scopeName": "source.msignore",
        "path": "./syntaxes/msignore.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "masterSVG.openPanel",
        "title": "%command.openPanel%",
        "icon": "$(symbol-color)"
      },
      {
        "command": "masterSVG.openWelcome",
        "title": "%command.openWelcome%",
        "icon": "$(home)"
      },
      {
        "command": "masterSVG.scanWorkspace",
        "title": "%command.scanWorkspace%"
      },
      {
        "command": "masterSVG.scanUsages",
        "title": "%command.scanUsages%",
        "icon": "$(references)"
      },
      {
        "command": "masterSVG.transformSvgToIcon",
        "title": "%command.transformSvgToIcon%"
      },
      {
        "command": "masterSVG.optimizeSvg",
        "title": "%command.optimizeSvg%",
        "icon": "$(rocket)"
      },
      {
        "command": "masterSVG.insertIcon",
        "title": "%command.insertIcon%"
      },
      {
        "command": "masterSVG.previewIcon",
        "title": "%command.previewIcon%"
      },
      {
        "command": "masterSVG.goToInlineSvg",
        "title": "%command.goToInlineSvg%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "masterSVG.goToUsage",
        "title": "%command.goToUsage%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "masterSVG.transformInlineSvg",
        "title": "%command.transformInlineSvg%",
        "icon": "$(replace)"
      },
      {
        "command": "masterSVG.configureProject",
        "title": "%command.configureProject%",
        "icon": "$(gear)"
      },
      {
        "command": "masterSVG.editIgnoreFile",
        "title": "%command.editIgnoreFile%",
        "icon": "$(list-filter)"
      },
      {
        "command": "masterSVG.refreshIcons",
        "title": "%command.refreshIcons%",
        "icon": "$(refresh)"
      },
      {
        "command": "masterSVG.refreshFiles",
        "title": "%command.refreshFiles%",
        "icon": "$(refresh)"
      },
      {
        "command": "masterSVG.refreshCode",
        "title": "%command.refreshCode%",
        "icon": "$(refresh)"
      },
      {
        "command": "masterSVG.refreshBuilt",
        "title": "%command.refreshBuilt%",
        "icon": "$(refresh)"
      },
      {
        "command": "masterSVG.forceRefreshPreview",
        "title": "%command.forceRefreshPreview%",
        "icon": "$(sync)"
      },
      {
        "command": "masterSVG.devRefreshAll",
        "title": "%command.devRefreshAll%",
        "icon": "$(sync-ignored)"
      },
      {
        "command": "masterSVG.buildIcons",
        "title": "%command.buildIcons%",
        "icon": "$(package)"
      },
      {
        "command": "masterSVG.searchIcons",
        "title": "%command.searchIcons%",
        "icon": "$(search)"
      },
      {
        "command": "masterSVG.deleteIcons",
        "title": "%command.deleteIcons%",
        "icon": "$(trash)"
      },
      {
        "command": "masterSVG.removeFromBuilt",
        "title": "%command.removeFromBuilt%",
        "icon": "$(remove)"
      },
      {
        "command": "masterSVG.showDetails",
        "title": "%command.showDetails%",
        "icon": "$(info)"
      },
      {
        "command": "masterSVG.goToCode",
        "title": "%command.goToCode%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "masterSVG.openSvgFile",
        "title": "%command.openSvgFile%",
        "icon": "$(file-media)"
      },
      {
        "command": "masterSVG.copyIconName",
        "title": "%command.copyIconName%",
        "icon": "$(copy)"
      },
      {
        "command": "masterSVG.colorEditor",
        "title": "%command.colorEditor%",
        "icon": "$(edit)"
      },
      {
        "command": "masterSVG.expandAll",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "masterSVG.collapseAll",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "masterSVG.expandBuiltIcons",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "masterSVG.collapseBuiltIcons",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "masterSVG.expandSvgFiles",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "masterSVG.collapseSvgFiles",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "masterSVG.configureSvgFolder",
        "title": "%command.configureSvgFolder%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "masterSVG.exportComponent",
        "title": "%command.exportComponent%",
        "icon": "$(file-code)"
      },
      {
        "command": "masterSVG.generateSprite",
        "title": "%command.generateSprite%",
        "icon": "$(layers)"
      },
      {
        "command": "masterSVG.addSvgToCollection",
        "title": "%command.addSvgToCollection%",
        "icon": "$(add)"
      },
      {
        "command": "masterSVG.importMissingIcon",
        "title": "%command.importMissingIcon%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "masterSVG.removeReference",
        "title": "%command.removeReference%",
        "icon": "$(trash)"
      },
      {
        "command": "masterSVG.findAndReplace",
        "title": "%command.findAndReplace%",
        "icon": "$(search-replace)"
      },
      {
        "command": "masterSVG.renameIcon",
        "title": "%command.renameIcon%",
        "icon": "$(edit)"
      },
      {
        "command": "masterSVG.viewSprite",
        "title": "%command.viewSprite%",
        "icon": "$(eye)"
      },
      {
        "command": "masterSVG.viewIconsFile",
        "title": "%command.viewIconsFile%",
        "icon": "$(eye)"
      },
      {
        "command": "masterSVG.deleteBuiltFile",
        "title": "%command.deleteBuiltFile%",
        "icon": "$(trash)"
      },
      {
        "command": "masterSVG.buildAllReferences",
        "title": "%command.buildAllReferences%",
        "icon": "$(package)"
      },
      {
        "command": "masterSVG.buildSingleIcon",
        "title": "%command.buildSingleIcon%",
        "icon": "$(package)"
      },
      {
        "command": "masterSVG.buildAllFiles",
        "title": "%command.buildAllFiles%",
        "icon": "$(package)"
      },
      {
        "command": "masterSVG.cleanSprite",
        "title": "%command.cleanSprite%",
        "icon": "$(trash)"
      },
      {
        "command": "masterSVG.generateLicenses",
        "title": "%command.generateLicenses%",
        "icon": "$(law)"
      },
      {
        "command": "masterSVG.showLicenseSummary",
        "title": "%command.showLicenseSummary%",
        "icon": "$(list-unordered)"
      },
      {
        "command": "masterSVG.openLibrary",
        "title": "Open Icon Library",
        "icon": "$(library)"
      },
      {
        "command": "masterSVG.searchLibrary",
        "title": "Search Icon Library",
        "icon": "$(search)"
      },
      {
        "command": "masterSVG.browseIconify",
        "title": "Browse Iconify",
        "icon": "$(globe)"
      },
      {
        "command": "masterSVG.toggleLibraryServer",
        "title": "Toggle Library Server",
        "icon": "$(server)"
      },
      {
        "command": "masterSVG.showLibraryServerOutput",
        "title": "Show Library Server Output",
        "icon": "$(output)"
      },
      {
        "command": "masterSVG.configureLibraryPath",
        "title": "Configure Library Path",
        "icon": "$(folder-opened)"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "masterSVG.language": {
          "type": "string",
          "enum": [
            "auto",
            "en",
            "es",
            "zh",
            "ru"
          ],
          "enumDescriptions": [
            "%config.language.auto%",
            "%config.language.en%",
            "%config.language.es%",
            "%config.language.zh%",
            "%config.language.ru%"
          ],
          "default": "auto",
          "description": "%config.language.description%"
        },
        "masterSVG.componentName": {
          "type": "string",
          "default": "svg-icon",
          "description": "%config.componentName.description%"
        },
        "masterSVG.componentImport": {
          "type": "string",
          "default": "@/components/ui/Icon",
          "description": "%config.componentImport.description%"
        },
        "masterSVG.svgFolders": {
          "type": "array",
          "default": [
            "svgs",
            "src/assets/icons",
            "src/icons",
            "public/icons",
            "assets/icons",
            "icons",
            "svg",
            "assets/svg"
          ],
          "description": "%config.svgFolders.description%"
        },
        "masterSVG.outputFormat": {
          "type": "string",
          "enum": [
            "jsx",
            "vue",
            "svelte",
            "astro",
            "html"
          ],
          "default": "jsx",
          "description": "Default output format for icon components"
        },
        "masterSVG.iconNameAttribute": {
          "type": "string",
          "default": "name",
          "description": "Attribute name for the icon identifier (name, icon, src, etc.)"
        },
        "masterSVG.autoImport": {
          "type": "boolean",
          "default": true,
          "description": "Automatically add import statement when inserting icons"
        },
        "masterSVG.libraryPath": {
          "type": "string",
          "default": "",
          "description": "Path to the Icon Studio library (icons.json)"
        },
        "masterSVG.outputDirectory": {
          "type": "string",
          "default": "",
          "description": "Directory where icons.js or sprite.svg will be generated (relative to workspace root)"
        },
        "masterSVG.buildFormat": {
          "type": "string",
          "enum": [
            "icons.js",
            "sprite.svg"
          ],
          "default": "icons.js",
          "description": "Output format for built icons"
        },
        "masterSVG.webComponentName": {
          "type": "string",
          "default": "",
          "description": "Name of the Web Component for icons (e.g. svg-icon)"
        },
        "masterSVG.deleteAfterBuild": {
          "type": "boolean",
          "default": false,
          "description": "Delete original SVG file after building to sprite/component"
        },
        "masterSVG.previewBackground": {
          "type": "string",
          "enum": [
            "transparent",
            "light",
            "dark",
            "checkered"
          ],
          "default": "checkered",
          "description": "Background style for icon preview"
        },
        "masterSVG.defaultIconSize": {
          "type": "number",
          "default": 24,
          "minimum": 8,
          "maximum": 512,
          "description": "Default size (in pixels) for generated icons"
        },
        "masterSVG.namingConvention": {
          "type": "string",
          "enum": [
            "kebab-case",
            "camelCase",
            "PascalCase",
            "snake_case"
          ],
          "default": "kebab-case",
          "description": "Naming convention for icon identifiers"
        },
        "masterSVG.svgoOptimize": {
          "type": "boolean",
          "default": true,
          "description": "Automatically optimize SVGs with SVGO when building"
        },
        "masterSVG.svgoRemoveViewBox": {
          "type": "boolean",
          "default": false,
          "description": "Remove viewBox attribute during SVGO optimization (not recommended)"
        },
        "masterSVG.svgoRemoveColors": {
          "type": "boolean",
          "default": false,
          "description": "Remove fill/stroke colors during optimization (uses currentColor)"
        },
        "masterSVG.defaultAnimation": {
          "type": "string",
          "enum": [
            "none",
            "spin",
            "pulse",
            "bounce",
            "shake",
            "fade"
          ],
          "default": "none",
          "description": "Default animation for new icons"
        },
        "masterSVG.showUsagesInTree": {
          "type": "boolean",
          "default": true,
          "description": "Show usage count next to icons in tree view"
        },
        "masterSVG.scanOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically scan workspace for SVG files on startup"
        },
        "masterSVG.licenseFormat": {
          "type": "string",
          "enum": [
            "combined",
            "perCollection",
            "both"
          ],
          "default": "combined",
          "description": "%config.licenseFormat.description%"
        },
        "masterSVG.autoGenerateLicenses": {
          "type": "boolean",
          "default": false,
          "description": "%config.autoGenerateLicenses.description%"
        },
        "masterSVG.licensesFolder": {
          "type": "string",
          "default": "icon-licenses",
          "description": "%config.licensesFolder.description%"
        },
        "masterSVG.excludePatterns": {
          "type": "array",
          "default": [
            "**/node_modules/**",
            "**/dist/**",
            "**/build/**",
            "**/.git/**"
          ],
          "description": "Glob patterns to exclude from SVG scanning"
        },
        "masterSVG.astroLibraryPath": {
          "type": "string",
          "default": "",
          "description": "Path to the Astro icon-manager project for browsing icons"
        },
        "masterSVG.astroLibraryPort": {
          "type": "number",
          "default": 4568,
          "minimum": 1024,
          "maximum": 65535,
          "description": "Port for the Astro icon library server"
        },
        "masterSVG.useAstroLibrary": {
          "type": "boolean",
          "default": true,
          "description": "Use Astro icon library instead of Iconify API for browsing icons"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "masterSVG",
          "title": "Icon Studio",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "masterSVG": [
        {
          "id": "masterSVG.svgFiles",
          "name": "%view.mastersvgFiles.title%"
        },
        {
          "id": "masterSVG.workspaceIcons",
          "name": "%view.mastersvgCode.title%"
        },
        {
          "id": "masterSVG.builtIcons",
          "name": "%view.mastersvgBuilt.title%"
        },
        {
          "id": "masterSVG.preview",
          "name": "%view.mastersvgPreview.title%",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "masterSVG.configureSvgFolder",
          "when": "view == masterSVG.svgFiles",
          "group": "navigation@-1"
        },
        {
          "command": "masterSVG.expandSvgFiles",
          "when": "view == masterSVG.svgFiles && !masterSVG.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.collapseSvgFiles",
          "when": "view == masterSVG.svgFiles && masterSVG.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.editIgnoreFile",
          "when": "view == masterSVG.svgFiles",
          "group": "navigation@1"
        },
        {
          "command": "masterSVG.buildAllFiles",
          "when": "view == masterSVG.svgFiles",
          "group": "navigation@2"
        },
        {
          "command": "masterSVG.refreshIcons",
          "when": "view == masterSVG.svgFiles",
          "group": "navigation@3"
        },
        {
          "command": "masterSVG.expandAll",
          "when": "view == masterSVG.workspaceIcons && !masterSVG.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.collapseAll",
          "when": "view == masterSVG.workspaceIcons && masterSVG.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.refreshIcons",
          "when": "view == masterSVG.workspaceIcons",
          "group": "navigation@1"
        },
        {
          "command": "masterSVG.buildAllReferences",
          "when": "view == masterSVG.workspaceIcons",
          "group": "navigation@2"
        },
        {
          "command": "masterSVG.expandBuiltIcons",
          "when": "view == masterSVG.builtIcons && !masterSVG.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.collapseBuiltIcons",
          "when": "view == masterSVG.builtIcons && masterSVG.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "masterSVG.refreshBuilt",
          "when": "view == masterSVG.builtIcons",
          "group": "navigation@1"
        },
        {
          "command": "masterSVG.searchIcons",
          "when": "view == masterSVG.builtIcons",
          "group": "navigation@2"
        },
        {
          "command": "masterSVG.configureProject",
          "when": "view == masterSVG.builtIcons",
          "group": "navigation@3"
        }
      ],
      "editor/context": [
        {
          "command": "masterSVG.transformSvgToIcon",
          "when": "editorHasSelection",
          "group": "masterSVG"
        },
        {
          "command": "masterSVG.optimizeSvg",
          "when": "editorHasSelection",
          "group": "masterSVG"
        },
        {
          "command": "masterSVG.previewIcon",
          "when": "editorTextFocus",
          "group": "masterSVG"
        }
      ],
      "explorer/context": [
        {
          "command": "masterSVG.scanWorkspace",
          "when": "explorerResourceIsFolder",
          "group": "masterSVG"
        },
        {
          "submenu": "masterSVG.svgMenu",
          "when": "resourceExtname == .svg",
          "group": "masterSVG"
        }
      ],
      "masterSVG.svgMenu": [
        {
          "command": "masterSVG.colorEditor",
          "when": "masterSVG.iconsJsExists",
          "group": "1_edit@1"
        },
        {
          "command": "masterSVG.showDetails",
          "group": "1_edit@2"
        }
      ],
      "view/item/context": [
        {
          "command": "masterSVG.addSvgToCollection",
          "when": "view == masterSVG.svgFiles && viewItem == svgIcon",
          "group": "inline"
        },
        {
          "command": "masterSVG.removeFromBuilt",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "inline"
        },
        {
          "command": "masterSVG.colorEditor",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/ && masterSVG.iconsJsExists",
          "group": "0_open@1"
        },
        {
          "command": "masterSVG.showDetails",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@2"
        },
        {
          "command": "masterSVG.openSvgFile",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "masterSVG.goToCode",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "masterSVG.copyIconName",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "masterSVG.exportComponent",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "masterSVG.renameIcon",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "7_modification"
        },
        {
          "command": "masterSVG.deleteIcons",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "masterSVG.removeFromBuilt",
          "when": "view == masterSVG.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "9_delete@2"
        },
        {
          "command": "masterSVG.transformInlineSvg",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef/",
          "group": "inline"
        },
        {
          "command": "masterSVG.goToInlineSvg",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|inlineSvgRasterized|inlineSvgRasterizedBuilt|inlineSvgBuilt|imgRef|imgRefRasterized/",
          "group": "0_open@1"
        },
        {
          "command": "masterSVG.colorEditor",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/ && masterSVG.iconsJsExists",
          "group": "0_open@2"
        },
        {
          "command": "masterSVG.showDetails",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "masterSVG.goToCode",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "masterSVG.copyIconName",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "masterSVG.exportComponent",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "masterSVG.deleteIcons",
          "when": "view == masterSVG.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "masterSVG.removeFromBuilt",
