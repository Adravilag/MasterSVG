{
  "name": "sagebox-icon-studio",
  "displayName": "SageBox Icon Studio",
  "description": "Professional SVG icon management studio. Preview, edit, transform and build icon components for your design system.",
  "version": "0.1.0",
  "publisher": "sagebox",
  "author": {
    "name": "Adrián Dávila Guerra",
    "url": "https://adravilag.github.io/sagebox/"
  },
  "license": "MIT",
  "homepage": "https://adravilag.github.io/sagebox/components/icon/",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "keywords": [
    "svg",
    "icons",
    "icon studio",
    "sagebox",
    "svg components",
    "iconify",
    "sprites",
    "react icons",
    "vue icons",
    "design system"
  ],
  "activationEvents": [
    "workspaceContains:**/*.svg",
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:svelte",
    "onLanguage:astro"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "iconWrap.svgMenu",
        "label": "Icon Studio"
      }
    ],
    "languages": [
      {
        "id": "iconwrapignore",
        "aliases": [
          "IconWrap Ignore"
        ],
        "filenames": [
          ".iconwrapignore"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "iconwrapignore",
        "scopeName": "source.iconwrapignore",
        "path": "./syntaxes/iconwrapignore.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "iconManager.openPanel",
        "title": "%command.openPanel%",
        "icon": "$(symbol-color)"
      },
      {
        "command": "iconManager.openWelcome",
        "title": "%command.openWelcome%",
        "icon": "$(home)"
      },
      {
        "command": "iconManager.scanWorkspace",
        "title": "%command.scanWorkspace%"
      },
      {
        "command": "iconManager.scanUsages",
        "title": "%command.scanUsages%",
        "icon": "$(references)"
      },
      {
        "command": "iconManager.transformSvgToIcon",
        "title": "%command.transformSvgToIcon%"
      },
      {
        "command": "iconManager.optimizeSvg",
        "title": "%command.optimizeSvg%",
        "icon": "$(rocket)"
      },
      {
        "command": "iconManager.insertIcon",
        "title": "%command.insertIcon%"
      },
      {
        "command": "iconManager.previewIcon",
        "title": "%command.previewIcon%"
      },
      {
        "command": "iconManager.goToInlineSvg",
        "title": "%command.goToInlineSvg%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "iconManager.goToUsage",
        "title": "%command.goToUsage%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "iconManager.transformInlineSvg",
        "title": "%command.transformInlineSvg%",
        "icon": "$(replace)"
      },
      {
        "command": "iconManager.configureProject",
        "title": "%command.configureProject%",
        "icon": "$(gear)"
      },
      {
        "command": "iconManager.editIgnoreFile",
        "title": "%command.editIgnoreFile%",
        "icon": "$(list-filter)"
      },
      {
        "command": "iconManager.refreshIcons",
        "title": "%command.refreshIcons%",
        "icon": "$(refresh)"
      },
      {
        "command": "iconManager.refreshFiles",
        "title": "%command.refreshFiles%",
        "icon": "$(refresh)"
      },
      {
        "command": "iconManager.refreshCode",
        "title": "%command.refreshCode%",
        "icon": "$(refresh)"
      },
      {
        "command": "iconManager.refreshBuilt",
        "title": "%command.refreshBuilt%",
        "icon": "$(refresh)"
      },
      {
        "command": "iconManager.buildIcons",
        "title": "%command.buildIcons%",
        "icon": "$(package)"
      },
      {
        "command": "iconManager.searchIcons",
        "title": "%command.searchIcons%",
        "icon": "$(search)"
      },
      {
        "command": "iconManager.deleteIcons",
        "title": "%command.deleteIcons%",
        "icon": "$(trash)"
      },
      {
        "command": "iconManager.removeFromBuilt",
        "title": "%command.removeFromBuilt%",
        "icon": "$(remove)"
      },
      {
        "command": "iconManager.showDetails",
        "title": "%command.showDetails%",
        "icon": "$(info)"
      },
      {
        "command": "iconManager.goToCode",
        "title": "%command.goToCode%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "iconManager.openSvgFile",
        "title": "%command.openSvgFile%",
        "icon": "$(file-media)"
      },
      {
        "command": "iconManager.copyIconName",
        "title": "%command.copyIconName%",
        "icon": "$(copy)"
      },
      {
        "command": "iconManager.colorEditor",
        "title": "%command.colorEditor%",
        "icon": "$(edit)"
      },
      {
        "command": "iconManager.expandAll",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "iconManager.collapseAll",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "iconManager.expandBuiltIcons",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "iconManager.collapseBuiltIcons",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "iconManager.expandSvgFiles",
        "title": "%command.expandAll%",
        "icon": "$(expand-all)"
      },
      {
        "command": "iconManager.collapseSvgFiles",
        "title": "%command.collapseAll%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "iconManager.configureSvgFolder",
        "title": "%command.configureSvgFolder%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "iconManager.exportComponent",
        "title": "%command.exportComponent%",
        "icon": "$(file-code)"
      },
      {
        "command": "iconManager.generateSprite",
        "title": "%command.generateSprite%",
        "icon": "$(layers)"
      },
      {
        "command": "iconManager.addSvgToCollection",
        "title": "%command.addSvgToCollection%",
        "icon": "$(add)"
      },
      {
        "command": "iconManager.removeReference",
        "title": "%command.removeReference%",
        "icon": "$(trash)"
      },
      {
        "command": "iconManager.findAndReplace",
        "title": "%command.findAndReplace%",
        "icon": "$(search-replace)"
      },
      {
        "command": "iconManager.renameIcon",
        "title": "%command.renameIcon%",
        "icon": "$(edit)"
      },
      {
        "command": "iconManager.viewSprite",
        "title": "%command.viewSprite%",
        "icon": "$(eye)"
      },
      {
        "command": "iconManager.viewIconsFile",
        "title": "%command.viewIconsFile%",
        "icon": "$(eye)"
      },
      {
        "command": "iconManager.deleteBuiltFile",
        "title": "%command.deleteBuiltFile%",
        "icon": "$(trash)"
      },
      {
        "command": "iconManager.buildAllReferences",
        "title": "%command.buildAllReferences%",
        "icon": "$(package)"
      },
      {
        "command": "iconManager.buildAllFiles",
        "title": "%command.buildAllFiles%",
        "icon": "$(package)"
      },
      {
        "command": "iconManager.cleanSprite",
        "title": "%command.cleanSprite%",
        "icon": "$(trash)"
      },
      {
        "command": "iconManager.generateLicenses",
        "title": "%command.generateLicenses%",
        "icon": "$(law)"
      },
      {
        "command": "iconManager.showLicenseSummary",
        "title": "%command.showLicenseSummary%",
        "icon": "$(list-unordered)"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "iconManager.language": {
          "type": "string",
          "enum": [
            "auto",
            "en",
            "es",
            "zh",
            "ru"
          ],
          "enumDescriptions": [
            "%config.language.auto%",
            "%config.language.en%",
            "%config.language.es%",
            "%config.language.zh%",
            "%config.language.ru%"
          ],
          "default": "auto",
          "description": "%config.language.description%"
        },
        "iconManager.componentName": {
          "type": "string",
          "default": "iw-icon",
          "description": "%config.componentName.description%"
        },
        "iconManager.componentImport": {
          "type": "string",
          "default": "@/components/ui/Icon",
          "description": "%config.componentImport.description%"
        },
        "iconManager.svgFolders": {
          "type": "array",
          "default": [
            "svgs",
            "src/assets/icons",
            "src/icons",
            "public/icons",
            "assets/icons",
            "icons",
            "svg",
            "assets/svg"
          ],
          "description": "%config.svgFolders.description%"
        },
        "iconManager.outputFormat": {
          "type": "string",
          "enum": [
            "jsx",
            "vue",
            "svelte",
            "astro",
            "html"
          ],
          "default": "jsx",
          "description": "Default output format for icon components"
        },
        "iconManager.iconNameAttribute": {
          "type": "string",
          "default": "name",
          "description": "Attribute name for the icon identifier (name, icon, src, etc.)"
        },
        "iconManager.autoImport": {
          "type": "boolean",
          "default": true,
          "description": "Automatically add import statement when inserting icons"
        },
        "iconManager.libraryPath": {
          "type": "string",
          "default": "",
          "description": "Path to the IconWrap library (icons.json)"
        },
        "iconManager.outputDirectory": {
          "type": "string",
          "default": "",
          "description": "Directory where icons.ts or sprite.svg will be generated (relative to workspace root)"
        },
        "iconManager.buildFormat": {
          "type": "string",
          "enum": [
            "icons.ts",
            "sprite.svg"
          ],
          "default": "",
          "description": "Output format for built icons"
        },
        "iconManager.webComponentName": {
          "type": "string",
          "default": "",
          "description": "Name of the Web Component for icons (e.g. sg-icon)"
        },
        "iconManager.deleteAfterBuild": {
          "type": "boolean",
          "default": false,
          "description": "Delete original SVG file after building to sprite/component"
        },
        "iconManager.previewBackground": {
          "type": "string",
          "enum": [
            "transparent",
            "light",
            "dark",
            "checkered"
          ],
          "default": "checkered",
          "description": "Background style for icon preview"
        },
        "iconManager.defaultIconSize": {
          "type": "number",
          "default": 24,
          "minimum": 8,
          "maximum": 512,
          "description": "Default size (in pixels) for generated icons"
        },
        "iconManager.namingConvention": {
          "type": "string",
          "enum": [
            "kebab-case",
            "camelCase",
            "PascalCase",
            "snake_case"
          ],
          "default": "kebab-case",
          "description": "Naming convention for icon identifiers"
        },
        "iconManager.svgoOptimize": {
          "type": "boolean",
          "default": true,
          "description": "Automatically optimize SVGs with SVGO when building"
        },
        "iconManager.svgoRemoveViewBox": {
          "type": "boolean",
          "default": false,
          "description": "Remove viewBox attribute during SVGO optimization (not recommended)"
        },
        "iconManager.svgoRemoveColors": {
          "type": "boolean",
          "default": false,
          "description": "Remove fill/stroke colors during optimization (uses currentColor)"
        },
        "iconManager.defaultAnimation": {
          "type": "string",
          "enum": [
            "none",
            "spin",
            "pulse",
            "bounce",
            "shake",
            "fade"
          ],
          "default": "none",
          "description": "Default animation for new icons"
        },
        "iconManager.showUsagesInTree": {
          "type": "boolean",
          "default": true,
          "description": "Show usage count next to icons in tree view"
        },
        "iconManager.scanOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically scan workspace for SVG files on startup"
        },
        "iconManager.licenseFormat": {
          "type": "string",
          "enum": [
            "combined",
            "perCollection",
            "both"
          ],
          "default": "combined",
          "description": "%config.licenseFormat.description%"
        },
        "iconManager.autoGenerateLicenses": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoGenerateLicenses.description%"
        },
        "iconManager.licensesFolder": {
          "type": "string",
          "default": "icon-licenses",
          "description": "%config.licensesFolder.description%"
        },
        "iconManager.excludePatterns": {
          "type": "array",
          "default": [
            "**/node_modules/**",
            "**/dist/**",
            "**/build/**",
            "**/.git/**"
          ],
          "description": "Glob patterns to exclude from SVG scanning"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "iconManager",
          "title": "Icon Studio",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "iconManager": [
        {
          "id": "iconManager.svgFiles",
          "name": "%view.iconwrapFiles.title%"
        },
        {
          "id": "iconManager.workspaceIcons",
          "name": "%view.iconwrapCode.title%"
        },
        {
          "id": "iconManager.builtIcons",
          "name": "%view.iconwrapBuilt.title%"
        },
        {
          "id": "iconManager.preview",
          "name": "%view.iconwrapPreview.title%",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "iconManager.configureSvgFolder",
          "when": "view == iconManager.svgFiles",
          "group": "navigation@-1"
        },
        {
          "command": "iconManager.expandSvgFiles",
          "when": "view == iconManager.svgFiles && !iconManager.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.collapseSvgFiles",
          "when": "view == iconManager.svgFiles && iconManager.svgFilesExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.editIgnoreFile",
          "when": "view == iconManager.svgFiles",
          "group": "navigation@1"
        },
        {
          "command": "iconManager.buildAllFiles",
          "when": "view == iconManager.svgFiles",
          "group": "navigation@2"
        },
        {
          "command": "iconManager.refreshIcons",
          "when": "view == iconManager.svgFiles",
          "group": "navigation@2"
        },
        {
          "command": "iconManager.expandAll",
          "when": "view == iconManager.workspaceIcons && !iconManager.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.collapseAll",
          "when": "view == iconManager.workspaceIcons && iconManager.workspaceExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.refreshIcons",
          "when": "view == iconManager.workspaceIcons",
          "group": "navigation@1"
        },
        {
          "command": "iconManager.buildAllReferences",
          "when": "view == iconManager.workspaceIcons",
          "group": "navigation@2"
        },
        {
          "command": "iconManager.expandBuiltIcons",
          "when": "view == iconManager.builtIcons && !iconManager.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.collapseBuiltIcons",
          "when": "view == iconManager.builtIcons && iconManager.builtExpanded",
          "group": "navigation@0"
        },
        {
          "command": "iconManager.refreshBuilt",
          "when": "view == iconManager.builtIcons",
          "group": "navigation@1"
        },
        {
          "command": "iconManager.searchIcons",
          "when": "view == iconManager.builtIcons",
          "group": "navigation@2"
        },
        {
          "command": "iconManager.configureProject",
          "when": "view == iconManager.builtIcons",
          "group": "navigation@3"
        }
      ],
      "editor/context": [
        {
          "command": "iconManager.transformSvgToIcon",
          "when": "editorHasSelection",
          "group": "iconManager"
        },
        {
          "command": "iconManager.optimizeSvg",
          "when": "editorHasSelection",
          "group": "iconManager"
        },
        {
          "command": "iconManager.previewIcon",
          "when": "editorTextFocus",
          "group": "iconManager"
        }
      ],
      "explorer/context": [
        {
          "command": "iconManager.scanWorkspace",
          "when": "explorerResourceIsFolder",
          "group": "iconManager"
        },
        {
          "submenu": "iconWrap.svgMenu",
          "when": "resourceExtname == .svg",
          "group": "iconManager"
        }
      ],
      "iconWrap.svgMenu": [
        {
          "command": "iconManager.colorEditor",
          "when": "iconManager.iconsJsExists",
          "group": "1_edit@1"
        },
        {
          "command": "iconManager.showDetails",
          "group": "1_edit@2"
        }
      ],
      "view/item/context": [
        {
          "command": "iconManager.addSvgToCollection",
          "when": "view == iconManager.svgFiles && viewItem == svgIcon",
          "group": "inline"
        },
        {
          "command": "iconManager.removeFromBuilt",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "inline"
        },
        {
          "command": "iconManager.colorEditor",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/ && iconManager.iconsJsExists",
          "group": "0_open@1"
        },
        {
          "command": "iconManager.showDetails",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@2"
        },
        {
          "command": "iconManager.openSvgFile",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "iconManager.goToCode",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "iconManager.copyIconName",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "iconManager.exportComponent",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "iconManager.renameIcon",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "7_modification"
        },
        {
          "command": "iconManager.deleteIcons",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIcon|svgIconBuilt|svgIconRasterized|svgIconRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "iconManager.removeFromBuilt",
          "when": "view == iconManager.svgFiles && viewItem =~ /svgIconBuilt|svgIconRasterizedBuilt/",
          "group": "9_delete@2"
        },
        {
          "command": "iconManager.transformInlineSvg",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef/",
          "group": "inline"
        },
        {
          "command": "iconManager.goToInlineSvg",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|inlineSvgRasterized|inlineSvgRasterizedBuilt|inlineSvgBuilt|imgRef|imgRefRasterized/",
          "group": "0_open@1"
        },
        {
          "command": "iconManager.colorEditor",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/ && iconManager.iconsJsExists",
          "group": "0_open@2"
        },
        {
          "command": "iconManager.showDetails",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@3"
        },
        {
          "command": "iconManager.goToCode",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "0_open@4"
        },
        {
          "command": "iconManager.copyIconName",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|builtIcon|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "1_copy@1"
        },
        {
          "command": "iconManager.exportComponent",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgBuilt/",
          "group": "1_copy@2"
        },
        {
          "command": "iconManager.deleteIcons",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvg|imgRef|inlineSvgRasterized|imgRefRasterized|inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "9_delete@1"
        },
        {
          "command": "iconManager.removeFromBuilt",
          "when": "view == iconManager.workspaceIcons && viewItem =~ /inlineSvgBuilt|inlineSvgRasterizedBuilt/",
          "group": "9_delete@2"
        },
        {
          "command": "iconManager.goToInlineSvg",
          "when": "view == iconManager.workspaceIcons && viewItem == missingRef",
          "group": "0_open@1"
        },
        {
          "command": "iconManager.removeReference",
          "when": "view == iconManager.workspaceIcons && viewItem == missingRef",
          "group": "1_fix@1"
        },
        {
          "command": "iconManager.findAndReplace",
          "when": "view == iconManager.workspaceIcons && viewItem == missingRef",
          "group": "1_fix@2"
        },
        {
          "command": "iconManager.colorEditor",
          "when": "view == iconManager.builtIcons && viewItem == builtIcon && iconManager.iconsJsExists",
          "group": "0_open@1"
        },
        {
          "command": "iconManager.showDetails",
          "when": "view == iconManager.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "0_open@2"
        },
        {
          "command": "iconManager.copyIconName",
          "when": "view == iconManager.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "1_copy@1"
        },
        {
          "command": "iconManager.exportComponent",
          "when": "view == iconManager.builtIcons && viewItem == builtIcon",
          "group": "1_copy@2"
        },
        {
          "command": "iconManager.renameIcon",
          "when": "view == iconManager.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "7_modification"
        },
        {
          "command": "iconManager.removeFromBuilt",
          "when": "view == iconManager.builtIcons && viewItem =~ /builtIcon|builtIconRasterized/",
          "group": "9_delete"
        },
        {
          "command": "iconManager.viewSprite",
          "when": "view == iconManager.builtIcons && viewItem == builtSpriteFile",
          "group": "0_open"
        },
        {
          "command": "iconManager.deleteBuiltFile",
          "when": "view == iconManager.builtIcons && viewItem == builtSpriteFile",
          "group": "9_delete"
        },
        {
          "command": "iconManager.viewIconsFile",
          "when": "view == iconManager.builtIcons && viewItem == builtIconsFile",
          "group": "0_open"
        },
        {
          "command": "iconManager.deleteBuiltFile",
          "when": "view == iconManager.builtIcons && viewItem == builtIconsFile",
          "group": "9_delete"
        }
      ]
    },
    "keybindings": [
      {
        "command": "iconManager.openPanel",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      },
      {
        "command": "iconManager.insertIcon",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node -e \"const fs=require('fs');const path=require('path');const dir='.vscode-test';if(fs.existsSync(dir))fs.rmSync(dir,{recursive:true,force:true})\" && npm run package:prod",
    "compile": "tsc -p ./ && npm run copy-templates",
    "compile:e2e": "tsc -p ./src/test/e2e/tsconfig.json && npm run copy-e2e-workspace",
    "copy-e2e-workspace": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)})};copyDir('src/test/e2e/test-workspace','out/test/e2e/test-workspace')\"",
    "copy-templates": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)})};copyDir('src/templates','out/templates')\"",
    "watch": "tsc -watch -p ./",
    "watch:esbuild": "node esbuild.js --watch",
    "package:prod": "node esbuild.js --production",
    "pretest": "npm run compile",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "npm run compile && npm run compile:e2e && node ./out/test/e2e/runTest.js",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
