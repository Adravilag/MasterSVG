{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Depurar Extensi√≥n (dist)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--disable-extensions"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js",
        "${workspaceFolder}/out/**/*.js"
      ],
      "sourceMaps": true,
      "preLaunchTask": "npm: watch"
    }
  ]
}
